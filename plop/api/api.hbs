import request from "@/utils/request";
/**
* 列表查询
* @param params
*/
export function get{{ upperFirst name }}List(params) {
  return request({
    url: "/{{ name }}/list",
    method: "get",
    params,
  });
}
/**
* 新增
* @param data
*/
export function do{{ upperFirst name }}Save(data) {
  return request({
    url: "/{{ name }}/save",
    method: "post",
    data,
  });
}
/**
* 修改
* @param data
*/
export function do{{ upperFirst name }}Update(data) {
  return request({
    url: "/{{ name }}/update",
    method: "post",
    data,
  });
}
/**
* 根据ID删除数据
* @param id
*/
export function do{{ upperFirst name }}Delete(id) {
  return request({
    url: "/{{ name }}/delete/"+id,
    method: "post",
  });
}
/**
* 根据ID查询单个数据
* @param id
*/
export function get{{ upperFirst name }}ById(id) {
  return request({
    url: "/{{ name }}/detail/"+id,
    method: "get",
  });
}
